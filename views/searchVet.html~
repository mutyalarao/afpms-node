<html>
<head>
<meta charset="utf-8">
<script src="/scripts/jquery-2.1.3.min.js"></script>
<script type='text/javascript'>
function getVet(){
$.ajax({url:'/api'
	,success:function (data) {
		var vets = [];
		$("#lbl1").text("success"+data[0].location);
		
		$.each(data, function (key,val) {
			vets.push("<h2 id='" + key + "'>" + val + "</h2>" );
			alert(val);
				});
				$( "<h1/>", {
				    "class": "my-new-list",
				    html: vets.join( "" )
				  }).appendTo( "body" );
				}		
		
	,error:function () {
	$("#lbl1").text("OOps");}	
	});
	}

</script>
<body>
    <div id="nav-bar">
        <a href = "/index.html">Home</a>        
    </div>
    <input type="text" id="vetId"/>
    <input type="button" onclick="getVet();"/>   
    <h1 id='lbl1'>NA</h1>
        
</body>

</html>
    